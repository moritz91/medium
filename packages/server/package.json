    "type-graphql": "0.17.5",
    "typedi": "0.8.0",
    "typeorm": "0.2.24",
    "typeorm-typedi-extensions": "0.2.3"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development tsnd -r tsconfig-paths/register --respawn --transpileOnly -- src/index.ts",
