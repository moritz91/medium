    "@types/passport-github": "1.1.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.7.5"
  },
  "dependencies": {
    "@medium/common": "1.0.0",
    "apollo-server-express": "2.9.3",
    "cors": "2.8.5",
    "cross-env": "^7.0.2",
    "dataloader": "1.4.0",
